<jxb:bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	jxb:version="2.1">

	<!-- bindings for commons -->
	<jxb:bindings schemaLocation="refereemanager.xsd">

		<!-- implementing classes -->
		<jxb:bindings node="//xsd:complexType[@name='Address']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.AddressModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Club']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.ClubModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Contact']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.ContactModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Content']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.ContentModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Person']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.PersonModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Referee']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.RefereeModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Season']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.SeasonModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='TitledIDType']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.TitledIDTypeModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='TitledType']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.TitledTypeModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='TrainingLevel']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.TrainingLevelModel" />
		</jxb:bindings>

		<jxb:bindings node="//xsd:complexType[@name='Wish']">
			<jxb:class implClass="de.edgesoft.refereemanager.model.WishModel" />
		</jxb:bindings>

	</jxb:bindings>

</jxb:bindings>

<!-- EOF -->

