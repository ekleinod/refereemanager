<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<SplitPane fx:id="pneSplit" dividerPositions="0.5" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.edgesoft.refereemanager.controller.overview.OverviewController">
	<items>
		<BorderPane fx:id="pneDetails" prefHeight="200.0" prefWidth="200.0">
			<top>
				<VBox spacing="5.0" BorderPane.alignment="TOP_LEFT">
					<children>
						<Label fx:id="lblHeading" text="Label" />
						<Separator orientation="VERTICAL" prefHeight="10.0" visible="false" />
						<fx:include fx:id="embeddedDetailsTitledID" source="../details/DetailsTitledID.fxml" />
					</children>
				</VBox>
			</top>
			<bottom>
				<fx:include fx:id="embeddedCRUDButtons" source="../CRUDButtons.fxml" />
			</bottom>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</BorderPane>
	</items>
</SplitPane>
