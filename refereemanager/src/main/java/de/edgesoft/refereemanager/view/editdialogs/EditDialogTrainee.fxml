<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.edgesoft.refereemanager.controller.editdialogs.EditDialogTraineeController">
	<children>
		<TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
			<tabs>
				<Tab closable="false" text="Personendaten">
					<content>
						<fx:include fx:id="embeddedInputFormPersonData" source="../inputforms/InputFormPersonData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Kontakt">
					<content>
						<fx:include fx:id="embeddedInputFormContactData" source="../inputforms/InputFormContactData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Adressen">
					<content>
						<fx:include fx:id="embeddedInputFormAddressData" source="../inputforms/InputFormAddressData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Schiri">
					<content>
						<fx:include fx:id="embeddedInputFormRefereeData" source="../inputforms/InputFormRefereeData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Wünsche">
					<content>
						<fx:include fx:id="embeddedInputFormWishData" source="../inputforms/InputFormWishData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Ausbildung">
					<content>
						<fx:include fx:id="embeddedInputFormTrainingLevelData" source="../inputforms/InputFormTrainingLevelData.fxml" />
					</content>
				</Tab>
				<Tab closable="false" text="Prüfung">
					<content>
						<fx:include fx:id="embeddedInputFormExamData" source="../inputforms/InputFormExamData.fxml" />
					</content>
				</Tab>
			</tabs>
		</TabPane>
		<ButtonBar AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
			<buttons>
				<Button fx:id="btnOK" defaultButton="true" onAction="#handleOk" text="_OK" />
				<Button fx:id="btnCancel" cancelButton="true" onAction="#handleCancel" text="A_bbrechen" />
			</buttons>
		</ButtonBar>
	</children>
</AnchorPane>
