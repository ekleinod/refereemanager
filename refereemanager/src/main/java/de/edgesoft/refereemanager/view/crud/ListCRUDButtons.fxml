<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.edgesoft.refereemanager.controller.crud.CRUDButtonsController">
	<children>
		<Button fx:id="btnAdd">
			<tooltip>
				<Tooltip text="Neu" />
			</tooltip>
		</Button>
		<Button fx:id="btnEdit">
			<tooltip>
				<Tooltip text="Ändern" />
			</tooltip>
		</Button>
		<Button fx:id="btnDelete">
			<tooltip>
				<Tooltip text="Löschen" />
			</tooltip>
		</Button>
	</children>
</VBox>
